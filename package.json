{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "cleanDocker": "docker system prune --filter until=24h -f",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma generate && npx prisma migrate dev --preview-feature",
    "db:seed": "npx ts-node prisma/seed.ts",
    "test:unitOnly": "nyc --reporter=lcovonly mocha -r ts-node/register test/**/*.test.ts --no-timeout --exit",
    "test:unit": "npm install && npx prisma migrate dev --name init --preview-feature && nyc --reporter=lcovonly mocha -r ts-node/register test/**/*.test.ts --no-timeout --exit",
    "lint": "npm install && eslint .",
    "pretty": "npx prettier --write .",
    "preinstall": "npx npm-force-resolutions",
    "build": "tsc --build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^2.15.0",
    "atob": "^2.1.2",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "@prisma/cli": "^2.15.0"
  },
  "devDependencies": {
    "cors": "^2.8.5",
    "@types/assert": "^1.5.4",
    "@types/atob": "^2.1.2",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.24",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "assert": "^2.0.0",
    "btoa": "^1.2.1",
    "eslint": "^7.7.0",
    "mocha": "^8.1.1",
    "npm-force-resolutions": "0.0.10",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "supertest": "^4.0.2"
  },
  "resolutions": {
    "lodash": "4.17.21"
  }
}
